{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": ["error", { "semi": true }]
  },

  "plugins": ["prettier"]
}
